  "version": "0.2.0",
  "configurations": [
    {
      "command": "yarn start",
      "name": "Cron Parser",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "Debug Jest Tests",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/node_modules/.bin/jest",
        "--runInBand"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
